{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "$": false
  },
  "plugins": ["html"],
  "settings": {
    "html/html-extensions": [".html", ".php"]
  },
  "extends": "airbnb",
  "rules": {
    // "spaced-comment": ["error", "always", { "exceptions": ["/"] }]
    "spaced-comment": "off"
  }
}