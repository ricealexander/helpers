{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "$": false
  },
  "extends": "airbnb",
  "rules": {
    // "spaced-comment": ["error", "always", { "exceptions": ["/"] }]
    "spaced-comment": "off",
    "key-spacing": "off",
    "linebreak-style": ["error", "unix"],
    "no-multi-spaces": "off"
  }
}